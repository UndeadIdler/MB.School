@using MB.School.Application.ViewModels
@model List<MB.School.Application.ViewModels.EnrollmentDateGroup>

@{
    ViewData["Title"] = "About";
}
<h2>学生信息统计</h2>
<table>
    <tr>
        <th>注册时间</th>
@*        <th>@Html.DisplayNameFor(Model.EnrollmentDate)</th>*@
        <th>学生数量</th>
    </tr>
    @foreach (var data in Model)
    {
        <tr>
            <td>@Html.DisplayFor(a=>data.EnrollmentDate)</td>
            <td>@Html.DisplayFor(a=>data.StudentCount)</td>
            
        </tr>

    }

</table>

<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Message"]</h3>

<p>Use this area to provide additional information.</p>
